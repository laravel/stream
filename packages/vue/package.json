{
    "name": "@laravel/stream-vue",
    "version": "0.3.8",
    "description": "Laravel streaming hooks for Vue",
    "keywords": [
        "laravel",
        "stream",
        "use-stream",
        "server-sent-events",
        "sse",
        "vue",
        "composables"
    ],
    "homepage": "https://github.com/laravel/stream/tree/main/packages/vue#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/laravel/stream.git"
    },
    "bugs": {
        "url": "https://github.com/laravel/stream/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Taylor Otwell"
    },
    "type": "module",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "lint": "eslint --config eslint.config.mjs \"src/**/*.ts\"",
        "prepublish": "pnpm run build",
        "release": "vitest --run && pnpm publish --no-git-checks",
        "test": "vitest",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@types/node": "^22.14.0",
        "@typescript-eslint/eslint-plugin": "^8.21.0",
        "@typescript-eslint/parser": "^8.21.0",
        "@vitejs/plugin-vue": "^5.0.0",
        "eslint": "^9.0.0",
        "jsdom": "^26.0.0",
        "msw": "^2.8.2",
        "prettier": "^3.5.3",
        "typescript": "^5.3.0",
        "vite": "^5.4.19",
        "vite-plugin-dts": "^4.5.3",
        "vitest": "^3.1.1"
    },
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "dependencies": {
        "nanoid": "^5.1.5"
    }
}
