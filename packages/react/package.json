{
    "name": "@laravel/stream-react",
    "version": "0.3.11",
    "description": "Laravel streaming hooks for React",
    "keywords": [
        "laravel",
        "stream",
        "use-stream",
        "server-sent-events",
        "sse",
        "react",
        "hooks"
    ],
    "homepage": "https://github.com/laravel/stream/tree/main/packages/react#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/laravel/stream.git"
    },
    "bugs": {
        "url": "https://github.com/laravel/stream/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Taylor Otwell"
    },
    "type": "module",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "vite build",
        "lint": "eslint --config eslint.config.mjs \"src/**/*.ts\"",
        "prepublish": "pnpm run build",
        "release": "vitest --run && pnpm publish --no-git-checks",
        "test": "vitest",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.2",
        "@types/node": "^22.14.0",
        "@types/react": "^19.2.0",
        "@typescript-eslint/eslint-plugin": "^8.56.0",
        "@typescript-eslint/parser": "^8.56.0",
        "@vitejs/plugin-vue": "^5.0.0",
        "eslint": "^10.0.0",
        "jsdom": "^26.0.0",
        "msw": "^2.12.0",
        "prettier": "^3.8.1",
        "typescript": "^5.9.0",
        "vite": "^5.4.0",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^3.1.0"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0"
    }
}
